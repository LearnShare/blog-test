{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "generate": {
      "dependsOn": []
    },
    "build": {
      "dependsOn": ["^build"]
    },
    "start": {
      "dependsOn": ["^build"]
    }
  }
}
